    "q": "^1.4.1",
    "superagent": "^3.0.0",
    "vue": "^2.2.2",
    "vue-i18n": "^5.0.3",
    "vue-router": "^2.2.0",
    "vuex": "^2.1.2",
    "vuex-persistedstate": "^1.4.1"
  },
